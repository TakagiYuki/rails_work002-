<h3>Editing Book</h3>
  <%= form_for(@book, url:"/books/#{@book.id}") do |f|%>
  <%= render 'shared/error_messages' %>
    Title<br>
      <p><%= f.text_field :title %></p>
    Body<br>
      <%= f.text_field :body %>
    <p><%= f.submit 'Update Book' %></p>
  <% end %>
<u><%= link_to "Show", book_path %></u> |
<u><%= link_to "Back", books_path %></u>
