<h1>New book</h1>
                        <!--create-->
    <%= form_for(@book,url:'/books') do |f| %>
         <%= render 'shared/error_messages' %>
       <p>Title</p>
       <%= f.text_field :title %>

       <p>Body</p>
       <%= f.text_area :body %>

       <p></p>
       <%= f.submit 'Create Book' %>
    <% end %>
